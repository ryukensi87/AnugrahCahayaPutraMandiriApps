rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /list_produk/{document} {
      allow create: if true;
      allow read: if true;
      allow write: if true;
      allow delete: if true;
    }

    match /list_mesin/{document} {
      allow create: if true;
      allow read: if true;
      allow write: if true;
      allow delete: if true;
    }
  }
}
